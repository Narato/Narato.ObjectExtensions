<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>Contains some utils extensions placed on String. A QOL library</Description>
    <AssemblyTitle>Narato.StringExtensions</AssemblyTitle>
    <NeutralLanguage>en-us</NeutralLanguage>
    <VersionPrefix>2.0.2</VersionPrefix>
    <Authors>Narato - http://www.narato.be</Authors>
    <TargetFrameworks>netstandard2.0;net46</TargetFrameworks>
    <AssemblyName>Narato.StringExtensions</AssemblyName>
    <PackageId>Narato.StringExtensions</PackageId>
    <PackageTags>Narato;Utils;StringExtensions</PackageTags>
    <PackageIconUrl>http://narato.be/wp-content/uploads/2016/11/narato-icon-logo-64x64-transparant.png</PackageIconUrl>
    <NetStandardImplicitPackageVersion>2.0.3</NetStandardImplicitPackageVersion>
    <AssetTargetFallback>$(AssetTargetFallback);dotnet5.6;dnxcore50;portable-net45+win8</AssetTargetFallback>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>
</Project>